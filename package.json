{
	"name": "time-folders",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo '' && bun run copy-pdfjs",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"copy-pdfjs": "mkdir -p static/pdfjs && cp -r node_modules/pdfjs-dist/build/* static/pdfjs && cp -r node_modules/pdfjs-dist/web/* static/pdfjs"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.32.0",
		"@sveltejs/adapter-node": "^5.2.13",
		"@sveltejs/kit": "^2.27.1",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/vite": "^4.1.11",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.37.3",
		"svelte-check": "^4.3.1",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.39.0",
		"vite": "^6.3.5"
	},
	"dependencies": {
		"pdfjs-dist": "^5.4.54"
	}
}
